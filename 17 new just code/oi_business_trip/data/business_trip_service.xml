		<field name="code">ticket</field>
		<field name="default" eval="True" />
	</record>

	<record id="service_car_travel" model="business.trip.service">
		<field name="name">Car Travel</field>
		<field name="code">car_travel</field>
	</record>
	
	<record id="service_car_rental" model="business.trip.service">
		<field name="name">Car Rental</field>
		<field name="code">car_rental</field>
	</record>	
	
	<record id="service_hotel" model="business.trip.service">
