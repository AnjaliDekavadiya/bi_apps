<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <template id="successful_review_custom_task" name="Success Page">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="oe_structure"/>
                    <div id="wrap">
                    <div class="oe_structure" id="oe_structure_website_form_contact_us_thanks_1"/>
                    <div class="container mt-4">
                        <div class="row">
                            <div class="col-lg-7 mr-lg-auto oe_structure">
                                <section class="pt40 s_text_block pb40 o_colored_level o_cc o_cc1" data-snippet="s_text_block" data-name="Text">
                                    <div class="container">
                                        <span class="d-block fa fa-4x fa-thumbs-up mx-auto rounded-circle bg-primary"/><br/>
                                        <h1 class="text-center">Thank You!</h1>
                                        <div class="pb16 pt16 s_hr" data-snippet="s_hr" data-name="Separator">
                                            <hr class="mx-auto border-top w-50 border-dark text-center"/>
                                        </div>
                                        <h5 class="text-left">
                                            <h2>Dear <t t-esc="user_id.name"/>,</h2>
                                            <br></br>
                                            <h2>Thank you for your time to do review and it has been recorded and sent successfully in the system.</h2>
                                            <br></br>
                                            <h2>Best regards,</h2>
                                            <h2>Project Team</h2>
                                        </h5>
                                    </div>
                                </section>
                            </div>
                        </div>
                    </div>
                    <div class="oe_structure" id="oe_structure_website_form_contact_us_thanks_2"/>
                  </div>
                <div class="oe_structure"/>
            </div>
        </t>
    </template>
    
