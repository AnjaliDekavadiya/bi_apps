<odoo>
	<data>
		<record id="walmart_feed_form" model="ir.ui.view">
			<field name="name">walmart.form.view</field>
			<field name="model">walmart.feed</field>
			<field name="priority" eval="1"/>
			<field name="arch" type="xml">
				<form>
					<group>
						<group>
							<field name="feed_id" placeholder="eg: 9F0F25D2EFEC486CA6118BA5293DAFC1@AQMBAgA" required="1"/>
						</group>
					</group>
					<footer>
						<button name="check_feed_status" string="Check Now" type="object" class="oe_highlight" />
                        <span>or </span>
                        <button special="cancel" string="Cancel" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</odoo>
