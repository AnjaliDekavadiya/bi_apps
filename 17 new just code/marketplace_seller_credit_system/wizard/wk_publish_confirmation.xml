<odoo>
    <data>
        <record id="wk_publish_confirmation" model="ir.ui.view">
            <field name="name">wk.publish.group.form</field>
            <field name="model">wk.publish.group</field>
            <field name="arch" type="xml">
                <form string="Confirm dialog">
                    <div>Only one Credit record can be publish for a website at a time.Do you want to unpublish the published record ?</div>
                    <footer>
                        <button class="oe_highlight" name="unpublish_group" string="Yes" type="object"/>
                        <button class="oe_highlight" name="do_nothing" string="No" type="object"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>