<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="custom_account_report_form_form_inherit" model="ir.ui.view">
            <field name="name">account.report.form.inherit.custom</field>
            <field name="model">account.report</field>
            <field name="inherit_id" ref="account_reports.account_report_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='filter_journals']" position="after">
                    <field name="filter_multi_branch"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>