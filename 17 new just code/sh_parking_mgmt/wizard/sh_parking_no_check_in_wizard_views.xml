<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="sh_parking_no_check_in_view" model="ir.ui.view">
            <field name="name">Parking Booking</field>
            <field name="model">sh.parking.no_check_in.wizard</field>
            <field name="arch" type="xml">
                <form string="Parking Booking">
                    <h5>The vehicle is already parked in this subslot.</h5>
                   
                    <footer>
                        <button string="Close" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="sh_parking_no_check_in_wizard_action" model="ir.actions.act_window">
            <field name="name">Parking Booking</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sh.parking.no_check_in.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>

        </record>

</odoo>