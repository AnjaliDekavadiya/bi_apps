<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Slot -->
        <record id="seq_sh_parking_slot" model="ir.sequence">
            <field name="name">Parking Slot</field>
            <field name="code">sh.parking.slot_no</field>
            <field name="prefix">ST</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
         <!-- Sequences for Subslot -->
         <record id="seq_sh_parking_subslot" model="ir.sequence">
            <field name="name">Parking Subslot</field>
            <field name="code">sh.parking.subslot_no</field>
            <field name="prefix">SS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for History -->
         <record id="seq_sh_parking_history" model="ir.sequence">
            <field name="name">Parking History</field>
            <field name="code">sh.parking.qrcode_no</field>
            <field name="prefix">MEMBER/PARKING/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>
