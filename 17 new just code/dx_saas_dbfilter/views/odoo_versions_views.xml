<odoo>
    <data>
        <record id="view_dx_saas_dbfilter_odoo_verion_tree" model="ir.ui.view">
            <field name="name">view.dx.saas.dbfilter.odoo.versions.tree</field>
            <field name="model">dx.saas.dbfilter.odoo.versions</field>
            <field name="arch" type="xml">
                <tree string="Odoo Versions" default_order="odoo_version desc">
                    <field name="odoo_version"/>
                </tree>
            </field>
        </record>
        <record id="action_dx_saas_dbfilter_odoo_verion" model="ir.actions.act_window">
            <field name="name">Odoo Versions</field>
            <field name="res_model">dx.saas.dbfilter.odoo.versions</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>