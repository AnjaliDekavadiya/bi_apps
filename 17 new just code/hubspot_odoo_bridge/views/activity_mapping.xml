<odoo>
	<data>
		<record id="form_hubspot_feed" model="ir.ui.view">
			<field name="name">channel.feed.from.hubspot</field>
			<field name="model">channel.feed</field>
			<field name="inherit_id" ref="odoo_multi_channel_crm.form_channel_feed"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='data']" position="after">
					<field name="activity_data" />
				</xpath>
			</field>
		</record>	  
	</data>
</odoo>
