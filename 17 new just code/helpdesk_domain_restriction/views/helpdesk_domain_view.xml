<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_helpdesk_domain_form" model="ir.ui.view">
            <field name="name">helpdesk.domain.form</field>
            <field name="model">helpdesk.domain</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name='name'/>
                    </group>
                </form>
            </field>
        </record>
        <record id="view_helpdesk_domain_tree" model="ir.ui.view">
            <field name="name">helpdesk.domain.tree</field>
            <field name="model">helpdesk.domain</field>
            <field name="arch" type="xml">
                <tree>
                    <field name='name'/>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="helpdesk_domain_view">
            <field name="name">Domain to Restrict</field>
            <field name="res_model">helpdesk.domain</field>
            <!-- <field name="view_type">form</field> --> <!-- odoo13 -->
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>