<odoo>
    <data>
        <!-- Home page template in Web page -->
        <template id="home_page_template" name="home Page">
            <t t-call="website.layout">
<!--                <link rel="stylesheet"-->
<!--                      href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css"/>-->
<!--                <link rel="stylesheet"-->
<!--                      href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css"/>-->
                <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<!--                <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>-->
                <script src="/pragtech_tailoring_management/static/src/js/feedback.js"></script>
                <div id="wrap" class="custom_structure custom_empty">
                    <section id="background_img"
                             class="custom_cover bg-black-50 pt96 custom_colored_level custom_parallax_no_overflow_hidden pb40 custom_img_bg vh-60"
                             data-scroll-background-ratio="0" data-snippet="custom_cover"
                             data-name="Cover" style=" margin-top: 100px;">
                        <div class="custom_allow_columns container2" id="background_img_text">
                            <h1 style="text-align: center;">
                                <font style="font-size: 62px; font-weight: bold;">"Experience the
                                    convenience of personalized tailoring brought to your location"
                                </font>
                            </h1>
                            <p class="lead" style="text-align: center;">
                                <br/>
                            </p>
                            <p style="text-align: center;">
                                <a href="/shop" class="btn btn-primary btn-lg mb-2"
                                   data-original-title="" title="" aria-describedby="tooltip926630">
                                    <font style="font-size: 24px;" data-original-title="" title=""
                                          aria-describedby="tooltip744592">BOOK NOW
                                    </font>
                                </a>
                            </p>
                        </div>
                    </section>

                    <!-- Mens Tailoring Section -->
                    <section class="custom_text_image pt-5 pb-5 custom_colored_level"
                             data-snippet="custom_image_text" data-name="Image - Text">
                        <div class="container2">
                            <div class="row align-items-center">
                                <div class="col-lg-6 pt-4 pb-4 custom_colored_level">
                                    <img
                                            src="https://e0.pxfuel.com/wallpapers/424/618/desktop-wallpaper-tovino-thomas.jpg"
                                            class="img img-fluid mx-auto" alt="Mens Tailoring"
                                            loading="lazy"/>
                                </div>
                                <div class="col-lg-6 pt-4 pb-4 custom_colored_level">
                                    <h2 class="text-dark">
                                        <b>MENS TAILORING</b>
                                    </h2>
                                    <p class="custom_default_snippet_text text-dark">Discover
                                        impeccable tailoring for men. Our expert tailors create
                                        custom-made outfits that exude style and comfort.
                                    </p>
                                    <p class="custom_default_snippet_text text-dark">Explore a wide
                                        range of fabrics and designs tailored to your preferences.
                                        Elevate your wardrobe with precision and craftsmanship.
                                    </p>
                                    <a t-attf-href="/shop/category/{{men}}" class="btn btn-primary">Explore
                                        Mens Tailoring
                                    </a>
                                </div>
                            </div>
                        </div>
                    </section>

                    <!-- Womens Tailoring Section -->
                    <section class="custom_text_image pt-5 pb-5 custom_colored_level"
                             data-snippet="custom_text_image" data-name="Text - Image"
                             style="margin-top :-80px">
                        <div class="container2">
                            <div class="row align-items-center">
                                <div class="col-lg-6 pt-4 pb-4 custom_colored_level">
                                    <h2 class="text-dark">
                                        <b>WOMENS TAILORING</b>
                                    </h2>
                                    <p class="custom_default_snippet_text text-dark">Experience the
                                        art of tailoring for women. Our skilled tailors create
                                        custom clothing that combines elegance and comfort.
                                    </p>
                                    <p class="custom_default_snippet_text text-dark">Choose from a
                                        variety of fabrics and designs tailored to your preferences.
                                        Redefine your style with our attention to detail.
                                    </p>
                                    <a t-attf-href="/shop/category/{{women}}" class="btn btn-primary">Explore
                                        Womens Tailoring
                                    </a>
                                </div>
                                <div class="col-lg-6 pt-4 pb-4 custom_colored_level">
                                    <img
                                            src="https://photos.telugurajyam.com/wp-content/uploads/2023/01/Aditi-Rao-Hydari-1-845x1024.jpg"
                                            class="img img-fluid mx-auto" alt="Womens Tailoring"
                                            loading="lazy"/>
                                </div>
                            </div>
                        </div>
                    </section>

                    <!-- Kids Tailoring Section -->
                    <section class="custom_text_image pt-5 pb-5 custom_colored_level"
                             data-snippet="custom_image_text" data-name="Image - Text"
                             style="margin-top :-80px">
                        <div class="container2">
                            <div class="row align-items-center">
                                <div class="col-lg-6 pt-4 pb-4 custom_colored_level">
                                    <img
                                            src="https://www.celebcrystal.com/wp-content/uploads/2021/12/Anahita-Hashemzadeh-scaled.jpg"  
                                            class="img img-fluid mx-auto" alt="Kids Tailoring"
                                            loading="lazy"/>
                                </div>
                                <div class="col-lg-6 pt-4 pb-4 custom_colored_level">
                                    <h2 class="text-dark">
                                        <b>KIDS TAILORING</b>
                                    </h2>
                                    <p class="custom_default_snippet_text text-dark">
                                        Our Kids Tailoring service ensures your little ones look
                                        their best. Our expert tailors create outfits that are both
                                        comfortable and stylish.
                                    </p>
                                    <p class="custom_default_snippet_text text-dark">
                                        Discover a range of designs and fabrics for your kids'
                                        clothing needs. Quality craftsmanship and a perfect fit,
                                        every time.
                                    </p>
                                    <a t-attf-href="/shop/category/{{kids}}" class="btn btn-primary">Explore
                                        Kids Tailoring
                                    </a>
                                </div>
                            </div>
                        </div>
                    </section>


                    <section id="how_it_works"
                             class="custom_process_steps pt-5 pb-5 custom_colored_level undefined custom_cc custom_cc1"
                             data-snippet="custom_process_steps" data-name="Steps"
                             style="position: relative; margin-top: 33px;margin-bottom: 100px;">
                        <div class="container2">
                            <div class="row no-gutters">
                                <div
                                        class="col-lg-3 custom_process_step pt-4 pb-4 custom_colored_level">
                                    <div class="custom_process_step_icon">
                                        <span class="icon-wrapper">
                                            <i
                                                    class="fa fa-shopping-basket fa-2x mx-auto rounded-circle bg-primary animated-icon"></i>
                                        </span>
                                    </div>
                                    <div class="custom_process_step_content text-center">
                                        <h3>CHOOSE STYLE</h3>
                                        <p>Choose your style</p>
                                    </div>
                                </div>
                                <div
                                        class="col-lg-3 custom_process_step pt-4 pb-4 custom_colored_level">
                                    <div class="custom_process_step_icon">
                                        <span class="icon-wrapper">
                                            <i
                                                    class="fa fa-unlock-alt fa-2x mx-auto rounded-circle bg-o-color-5 animated-icon"></i>
                                        </span>
                                    </div>
                                    <div class="custom_process_step_content text-center">
                                        <h3>GET MEASURED</h3>
                                        <p>Give the measurement</p>
                                    </div>
                                </div>
                                <div
                                        class="col-lg-3 custom_process_step pt-4 pb-4 custom_colored_level">
                                    <div class="custom_process_step_icon">
                                        <span class="icon-wrapper">
                                            <i
                                                    class="fa fa-paypal fa-2x mx-auto rounded-circle bg-secondary animated-icon"></i>
                                        </span>
                                    </div>
                                    <div class="custom_process_step_content text-center">
                                        <h3>Pay</h3>
                                        <p>Do the Payment</p>
                                    </div>
                                </div>
                                <div
                                        class="col-lg-3 custom_process_step pt-4 pb-4 custom_colored_level">
                                    <div class="custom_process_step_icon">
                                        <span class="icon-wrapper">
                                            <i
                                                    class="fa fa-plane fa-2x mx-auto rounded-circle bg-o-color-3 animated-icon"></i>
                                        </span>
                                    </div>
                                    <div class="custom_process_step_content text-center">
                                        <h3>Get Delivered</h3>
                                        <p>Delivered within 10-12 days</p>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </section>

                    <!--                    <section class="style="margin-top :-80px" pt-5 pb-5 custom_colored_level"-->
                    <!--                        data-snippet="custom_text_block" data-name="Text">-->
                    <!--                        <div class="custom_allow_columns container2">-->
                    <!--                            <div class="owl-carousel" id="testimonial-carousel">-->
                    <!--                            </div>-->
                    <!--                        </div>-->
                    <!--                    </section>-->
                </div>
            </t>
        </template>
    </data>
</odoo>