<?xml version="1.0"?>
<odoo>
	<data>
		<record id="service_action_type_form" model="ir.ui.view">
			<field name="name">service.action.type.form</field>
			<field name="model">service.action.type</field>
			<field name="arch" type="xml">
				<form string="Action Type">
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="service_action_type_tree" model="ir.ui.view">
			<field name="name">service.action.type.tree</field>
			<field name="model">service.action.type</field>
			<field name="arch" type="xml">
				<tree string="Action Type">
					<field name="name" />
				</tree>
			</field>
		</record>

		<record id="action_service_action_type" model="ir.actions.act_window">
			<field name="name">Action Type</field>
			<field name="res_model">service.action.type</field>
			<field name="view_mode">tree,form</field>
		</record>
		
		
	</data>
</odoo>